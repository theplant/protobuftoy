{
    "compilerOptions": {
        "jsx": "react-native",
        "target": "esnext",
        "strictNullChecks": true,
        "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "pretty": true,
        "moduleResolution": "node",
        "allowJs": true,
        "outDir": "build"
    },
    "include": [
        "*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}
